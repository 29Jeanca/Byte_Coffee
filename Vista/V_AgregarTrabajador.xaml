<Window x:Class="Byte_Coffee.Vista.V_AgregarTrabajador"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Byte_Coffee.Vista"
        mc:Ignorable="d"
        Title="Agregar trabajador"
        Height="450"
        Width="800"
        Background='#000'>
    <Grid>
        <TextBlock x:Name='txtNombre' />
        <TextBox x:Name='txtApellido1' />
        <TextBox x:Name='txtApellido2' />
        <TextBox x:Name='Correo' />
        <TextBox x:Name='Puesto' />
        <TextBox x:Name='Horario' />
        <TextBox x:Name='Salario' />
    </Grid>
</Window>
